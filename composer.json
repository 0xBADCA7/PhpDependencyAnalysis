{
    "name"       : "mamuz/php-dependency-analysis",
    "type"             : "library",
    "description": "Static code analysis for php projects to provide a dependency graph based on namespaces",
    "homepage"         : "https://github.com/mamuz/PhpDependencyAnalysis",
    "license"          : "MIT",
    "keywords"   : ["mamuz", "php", "dependency", "analysis", "graph", "namespace"],
    "authors"          : [
        {
            "name" : "Marco Muths",
            "email": "muzzi_is@web.de"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable"    : true,
    "require"          : {
        "php"             : ">=5.4",
        "nikic/php-parser": "1.0.0beta2",
        "clue/graph"      : "0.7.*",
        "symfony/yaml"    : "2.5.*",
        "symfony/console" : "2.5.*",
        "symfony/finder"  : "2.5.*"
    },
    "require-dev"      : {
        "satooshi/php-coveralls": "0.6.*",
        "phpunit/phpunit"       : "4.2.*",
        "mockery/mockery"       : "0.9.*"
    },
    "autoload"         : {
        "psr-4": {
            "PhpDA\\": "src/"
        }
    },
    "bin"              : ["bin/phpda"]
}
